{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@*": ["src/*"],
      "@components": ["src/components"],
      "@navigations": ["src/navigations"],
      "@api": ["src/api"],
      "@assets": ["src/assets"],
      "@screens": ["src/screens"],
      "@utils": ["src/utils"],
      "@redux": ["src/redux"],
      "@appheme": ["src/theme"],
      "@hooks": ["src/hooks"]
    }
  },
  "exclude": ["node_modules"]
}
